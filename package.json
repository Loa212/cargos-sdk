{
  "name": "cargos-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for Italian State Police CARGOS car rental contract submission service",
  "type": "module",
  "main": "./dist/cargos-sdk.js",
  "module": "./dist/cargos-sdk.js",
  "types": "./dist/cargos-sdk.d.ts",
  "exports": {
    ".": {
      "types": "./dist/cargos-sdk.d.ts",
      "import": "./dist/cargos-sdk.js",
      "require": "./dist/cargos-sdk.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun build ./src/cargos-sdk.ts --outdir dist --format esm --declaration && bun build ./src/cargos-sdk.ts --outdir dist --format cjs --outfile cargos-sdk.cjs",
    "build:esm": "bun build ./src/cargos-sdk.ts --outdir dist --format esm --declaration",
    "build:cjs": "bun build ./src/cargos-sdk.ts --outdir dist --format cjs --outfile cargos-sdk.cjs",
    "build:watch": "bun build ./src/cargos-sdk.ts --outdir dist --format esm --declaration --watch",
    "dev": "bun run ./src/cargos-example.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "biome lint src",
    "lint:fix": "biome lint --write src",
    "format": "biome format --write src",
    "check": "biome check src",
    "check:fix": "biome check --write src",
    "type-check": "tsc --noEmit",
    "ci": "bun run check && bun run type-check && bun run build && bun test"
  },
  "keywords": [
    "cargos",
    "polizia",
    "italy",
    "rental",
    "contracts",
    "italian-state-police",
    "terrorism-prevention",
    "api",
    "sdk",
    "typescript",
    "bun",
    "biome"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/cargos-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/cargos-sdk/issues"
  },
  "homepage": "https://github.com/yourusername/cargos-sdk#readme",
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@biomejs/cli-darwin-arm64": "^2.3.14",
    "@types/bun": "latest",
    "typescript": "^5.4.0"
  },
  "peerDependencies": {},
  "engines": {
    "bun": ">=1.0.0"
  }
}